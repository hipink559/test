<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">

<link rel="stylesheet" href="./css/style.css">

<title>ヘッダー</title>

</head>
<body>
	<header>

		<div id="header">


			<!-- ヘッダーロゴ -->
  			<div id="header-logo">
 			<a href="home.jsp">
 					<img src="images/logo.jpg"/>
 								</a>

			</div>

			<div id="header-menu">
				<ul>
					<s:form action="SearchItemAction">

					<!-- カテゴリーリストを出す -->
						<s:if test='#session.containsKey("mCategoryDtoList")'>
							<li><s:select name="categoryId"
									list="#session.mCategoryDtoList" listValue="categoryName"
									listKey="categoryId" class="cs-div" id="categoryId" /></li>
						</s:if>

						<!-- キーワード検索 -->
						<li><s:textfield name="keywords" class="txt-keywords"
								placeholder="検索ワード" /></li>
						<li>
							<s:submit type="image" src="data:image/svg+xml;utf8;base64,"/>
 						</li>

 						<!-- ログアウト -->
						<s:if test="#session.logined==1">
							<li id="icon"><a href=<s:url action="LogoutAction"/>>
							<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_logout.jpg" id="active"/>
 								</a>
 								</li>
						</s:if>

						<!-- ログイン -->
						<s:else>
							<li id="icon"><a href="GoLoginAction">
							<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_login.jpg" id="active"/>
 								</a>
 								</li>
						</s:else>

						<!-- カート -->
 						<li id="icon"><a href=<s:url action="CartAction"/>>
 						<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_cart.jpg" id="active"/>
 								</a>
 								</li>

 						<!-- 商品リスト -->
 						<li id="icon"><a href=<s:url action="ProductListAction"/>>
 						<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_list.jpg" id="active"/>
 								</a>
 								</li>

						<!-- 管理者ページ -->
						<s:if test="#session.status == 1">
							<li id="icon"><a href=<s:url action="AdminAction"/>>
								<img src="data:image/svg+xml;utf8;base64," />
								<img src="./images/icon_admin.jpg" id="active"/>
								</a>
 								</li>
						</s:if>


						<!-- マイページ -->
						<s:elseif test="#session.logined==1">
							<li id="icon"><a href=<s:url action="MyPageAction"/>>
							<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_mypage.jpg" id="active"/>
 								</a>
 								</li>
						</s:elseif>

						<li></li>
						<li></li>
					</s:form>


				</ul>
			</div>
		</div>
		<div class="clearfix">
		</div>
	</header>
</body>
</html>